{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Poetry: Run 'run.py'",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/run.py",
      "console": "integratedTerminal",
      "python": "${command:python.interpreterPath}",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}